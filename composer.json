{
    "name": "jarl/activity-log",
    "description": "基于laravel-activitylog的操作日志扩展",
    "type": "library",
    "keywords": ["laravel", "dcat-admin", "extension", "activity-log"],
    "homepage": "https://github.com/jarl/activity-log",
    "license": "MIT",
    "authors": [
        {
            "name": "jarl",
            "email": "1076999758@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "dcat/laravel-admin": "~2.0",
        "spatie/laravel-activitylog": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Jarl\\ActivityLog\\": "src/"
        }
    },
    "extra": {
        "dcat-admin": "Jarl\\ActivityLog\\ActivityLogServiceProvider",
        "laravel": {
            "providers": [
                "Jarl\\ActivityLog\\ActivityLogServiceProvider"
            ]
        }
    }
}
